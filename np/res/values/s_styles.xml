<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="s_im_button">
		<item name="android:background">@drawable/s_btn_default_bg</item>
	</style>
	<style name="s_im_popup_number_button" parent="@android:style/Widget.Button">
		<item name="android:layout_width">95sp</item>
		<item name="android:layout_height">70sp</item>
		<item name="android:background">@drawable/s_btn_default_bg</item>
	</style>
	<style name="s_im_popup_note_number_button" parent="@android:style/Widget.Button.Toggle">
		<item name="android:layout_width">95sp</item>
		<item name="android:layout_height">70sp</item>
		<item name="android:background">@drawable/s_btn_toggle_bg</item>
	</style>
	<style name="s_im_popup_close_button" parent="@android:style/Widget.Button">
		<item name="android:layout_width">140sp</item>
		<item name="android:layout_height">60sp</item>
		<item name="android:layout_marginTop">10sp</item>
		<item name="android:background">@drawable/s_btn_default_bg</item>
	</style>
	<style name="s_im_popup_clear_button" parent="@android:style/Widget.Button">
		<item name="android:layout_width">95sp</item>
		<item name="android:layout_height">60sp</item>
		<item name="android:layout_marginTop">10sp</item>
		<item name="android:layout_marginLeft">45sp</item>
		<item name="android:background">@drawable/s_btn_default_bg</item>
	</style>

	<!-- themes -->
	<style name="s_Theme" parent="android:Theme">
	</style>

	<style name="s_Theme.Default">
		<item name="s_lineColor">#666666</item>
		<item name="s_sectorLineColor">@android:color/black</item>
		<item name="s_textColor">@android:color/black</item>
		<item name="s_textColorReadOnly">@android:color/black</item>
		<item name="s_textColorNote">@android:color/black</item>
		<item name="s_backgroundColor">@android:color/white</item>
		<item name="s_backgroundColorReadOnly">#cccccc</item>
		<item name="s_backgroundColorTouched">#3232ff</item>
		<item name="s_backgroundColorSelected">#ffff00</item>
	</style>
	<style name="s_Theme.PaperI">
		<item name="s_lineColor">#666666</item>
		<item name="s_sectorLineColor">@android:color/black</item>
		<item name="s_textColor">#555555</item>
		<item name="s_textColorReadOnly">@android:color/black</item>
		<item name="s_textColorNote">#555555</item>
		<item name="s_backgroundColor">@android:color/white</item>
		<item name="s_backgroundColorTouched">#3232ff</item>
		<item name="s_backgroundColorSelected">#ffff00</item>
	</style>
	<style name="s_Theme.PaperII">
		<item name="s_lineColor">#666666</item>
		<item name="s_sectorLineColor">@android:color/black</item>
		<item name="s_textColor">#555555</item>
		<item name="s_textColorReadOnly">@android:color/black</item>
		<item name="s_textColorNote">#555555</item>
		<item name="s_backgroundColor">#dddddd</item>
		<item name="s_backgroundColorSecondary">@android:color/white</item>
		<item name="s_backgroundColorTouched">#3232ff</item>
		<item name="s_backgroundColorSelected">#ffff00</item>
	</style>
</resources>
